.inner
  %table.table
    %tr
      %th.first
        = t("Class")
      %th
        = t("Model")
      %th
        = t("Parent")
    - @classnames.each do |cn|
      - c = CimClass.find_by_name(cn)
      - STDERR.puts "Can't find class '#{cn}' - did you mof-populate ?!" unless c
      %tr{:class => cycle("odd", "even")}
        %td
          = link_to cn, instances_path(:class => c)
        %td
          = c.cim_model.name rescue ""
        %td
          = c.parent.name rescue ""
        %td.last
  .actions-bar.wat-cf
    .actions
    = paginate @classnames
